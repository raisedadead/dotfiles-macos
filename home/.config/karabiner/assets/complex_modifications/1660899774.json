{
  "title": "Sane Home and End Keys",
  "rules": [
    {
      "description": "Home to Cmd + Left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": [
                "left_gui",
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": "left_gui"
            }
          ]
        }
      ]
    },
    {
      "description": "Home to Cmd + Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "optional": [
                "left_gui",
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": "left_gui"
            }
          ]
        }
      ]
    },
    {
      "description": "Shift + Home to Shift + Cmd + Left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "mandatory": ["left_shift"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["shift", "left_gui"]
            }
          ]
        }
      ]
    },
    {
      "description": "Shift + End to Shift + Cmd + Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "shift",
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Cmd + Shift + Home to Shift + Home",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "mandatory": [
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "home",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Cmd + Shift + End to Shift + End",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "mandatory": [
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "end",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}
